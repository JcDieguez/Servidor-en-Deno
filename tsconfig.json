{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["es6", "dom"],
      "jsx": "react",
      "esModuleInterop": true,
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*", "src/types/*"]
      }
    },
    "exclude": ["node_modules"]
  }
  